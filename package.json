{
    "name": "newo",
    "version": "1.4.0",
    "description": "NEWO CLI: sync flows/skills between NEWO and local files, multi-project support, import AKB articles",
    "type": "module",
    "bin": {
        "newo": "src/index.ts"
    },
    "files": [
        "src/**/*.ts",
        "README.md",
        "CHANGELOG.md",
        ".env.example"
    ],
    "keywords": [
        "newo",
        "cli",
        "ai",
        "agent",
        "automation",
        "sync",
        "local-development",
        "akb",
        "knowledge-base",
        "import",
        "multi-project",
        "workspace"
    ],
    "author": "sabbah13, constfilin",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/constfilin/newo-cli.git"
    },
    "bugs": {
        "url": "https://github.com/constfilin/newo-cli/issues"
    },
    "homepage": "https://github.com/constfilin/newo-cli#readme",
    "engines": {
        "node": ">=18"
    },
    "dependencies": {
        "@google-cloud/bigquery": "^8.1.1",
        "axios": "^1.7.7",
        "command-line-args": "^6.0.1",
        "command-line-usage": "^7.0.3",
        "console.table": "^0.10.0",
        "dayjs": "^1.11.18",
        "dotenv": "^16.4.5",
        "fs-extra": "^11.2.0",
        "googleapis": "^159.0.0",
        "http": "^0.0.1-security",
        "js-yaml": "^4.1.0",
        "jwt-decode": "^4.0.0",
        "objects-to-csv": "^1.3.6",
        "openai": "^6.0.1",
        "tsx": "^4.20.5"
    },
    "devDependencies": {
        "@types/fs-extra": "^11.0.4",
        "@types/js-yaml": "^4.0.9",
        "@types/node": "^24.6.2",
        "mocha": "^10.2.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3"
    },
    "scripts": {
        "help": "npx tsx ./src/index.ts help",
        "pullProjects": "npx tsx ./src/index.ts pullProjects",
        "listProjectMetas": "npx tsx ./src/index.ts listProjectMetas",
        "getCustomerProfile": "npx tsx ./src/index.ts getCustomerProfile",
        "getAttributes": "npx tsx ./src/index.ts getAttributes",
        "getSessions": "npx tsx ./src/index.ts getSessions -v"
    }
}
